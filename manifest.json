{
  "manifest_version": 3,
  "name": "Airtime",
  "version": "2.1.0",
  "description": "Your digital headquarters. Track focus, analyze insights, and audit your time with Airtime.",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "idle",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Airtime"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}